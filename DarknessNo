--// Load Spawner
local spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/Voor-Pr00/Fogroom/refs/heads/main/Spawner%20v3"))()
if not spawner or not spawner.Create then
    warn("Spawner không tải được hoặc thiếu hàm Create!")
    return
end

--// Tạo entity
local entity = spawner.Create({
	Entity = {
		Name = "Darkness",
		Asset = "rbxassetid://121725853188313",
		HeightOffset = 0
	},
	Lights = {
		Flicker = {
			Enabled = false,
			Duration = 4
		},
		Shatter = true,
		Repair = false
	},
	Light = {
		Color = {1, 1, 1}
	},
	Earthquake = { Enabled = false },
	CameraShake = {
		Enabled = false,
		Range = 100,
		Values = {2.5, 35, 0.15, 2}
	},
	Movement = {
		Speed = 15,
		Delay = 0,
		Reversed = false
	},
	Rebounding = {
		Enabled = false,
		Type = "Ambush",
		Min = 1,
		Max = 1,
		Delay = 2
	},
	Damage = {
		Enabled = true,
		Range = 30,
		Amount = 0.01
	},
	Crucifixion = {
		Enabled = true,
		Range = 40,
		Resist = false,
		Break = true
	},
	Death = {
		Type = "Guiding",
		Hints = {
			"Welcome back, what did you get now?",
			"Hm.. I don't really know who you dead to",
			"Maybe you can call him Darkness",
			"Whenever he appears, you will hear the wind noises",
			"Anyways you can do it! Bye!! :>"
		},
		Cause = ""
	}
})

--// Callbacks
entity:SetCallback("OnSpawned", function()
    print("Entity has spawned")
local function GitAud(soundgit, filename)
    local url = soundgit
    local FileName = filename
    writefile(FileName .. ".mp3", game:HttpGet(url))
    return (getcustomasset or getsynasset)(FileName .. ".mp3")
end

local function CustomGitSound(soundlink, vol, filename)
    local sound = Instance.new("Sound")
    sound.SoundId = GitAud(soundlink, filename)
    sound.Parent = workspace
    sound.Volume = 1
    sound.PlaybackSpeed = 1
    sound.Name = "Ambient"
    sound:Play()
end
	CustomGitSound("https://github.com/nervehammer1/throwawaystuff/raw/refs/heads/main/NightmareAmbient.mp3?raw=true", 1, "Amb")

    local foot = workspace:FindFirstChild("Darkness") and workspace.Darkness:FindFirstChild("RushNew")
    if foot and foot:FindFirstChild("Footsteps") then
        game.Workspace.Darkness.RushNew.Footsteps:Destroy()
    end
end)

entity:SetCallback("OnStartMoving", function()
    print("Entity has started moving")
end)

entity:SetCallback("OnEnterRoom", function(room, firstTime)
    if firstTime then
        print("Entity has entered room: " .. room.Name .. " for the first time")
    else
        print("Entity has entered room: " .. room.Name .. " again")
    end
end)

entity:SetCallback("OnLookAt", function(lineOfSight)
	if lineOfSight then
		print("Player is looking at entity")
		if workspace:FindFirstChild("Darkness") then
			workspace.Darkness:Destroy()
     workspace.Ambient:Destroy()
		end
	else
		print("Player view is obstructed")
	end
end)

entity:SetCallback("OnRebounding", function(startOfRebound)
    if startOfRebound then
        print("Entity has started rebounding")
	else
        print("Entity has finished rebounding")
	end
end)

entity:SetCallback("OnDespawning", function()
    print("Entity is despawning")
end)

entity:SetCallback("OnDespawned", function()
    print("Entity has despawned")
game.Workspace.Ambient:Destroy()
end)

entity:SetCallback("OnDamagePlayer", function(newHealth)
	if newHealth == 0 then
		print("Entity has killed the player")
	else
		print("Entity has damaged the player")
game Workspace.Ambient:Destroy()
game.Workspace.Darkness:Destroy()
		if workspace:FindFirstChild("Darkness") then
--// Jumpscare Entity Script for Doors
-- Paste this into executor

-- Settings
local entityImageIdle = "rbxassetid://122446161872338"            -- ID khi thực thể xa
local entityImageJumpscare = "rbxassetid://115667019397281"  -- ID khi jumpscare
local jumpSound = "rbxassetid://130990524400761"             -- ID âm thanh jumpscare

-- Create ScreenGui
local gui = Instance.new("ScreenGui")
gui.Name = "JumpscareGui"
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Background
local bg = Instance.new("Frame")
bg.Size = UDim2.new(1, 0, 1, 0)
bg.BackgroundColor3 = Color3.new(1, 1, 1) -- trắng
bg.BackgroundTransparency = 0
bg.Parent = gui

-- Entity Image
local img = Instance.new("ImageLabel")
img.Size = UDim2.new(0.3, 0, 0.3, 0) -- Nhỏ xa
img.Position = UDim2.new(0.35, 0, 0.35, 0)
img.BackgroundTransparency = 1
img.Image = entityImageIdle
img.Parent = gui

-- Sound (chỉ phát khi jumpscare)
local sound = Instance.new("Sound")
sound.SoundId = jumpSound
sound.Volume = 1
sound.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Camera Shake Effect
local RunService = game:GetService("RunService")
local shaking = true
local intensity = 2

RunService.RenderStepped:Connect(function()
	if shaking then
		img.Position = UDim2.new(
			0.35, math.random(-intensity, intensity),
			0.35, math.random(-intensity, intensity)
		)
	end
end)

-- Stage 1: xa và rung nhẹ
intensity = 2
task.wait(5)

-- Stage 2: Jumpscare (chỉ 1 giây)
img.Image = entityImageJumpscare
img:TweenSize(UDim2.new(0.9, 0, 0.9, 0), "Out", "Quad", 0.3, true)
img:TweenPosition(UDim2.new(0.05, 0, 0.05, 0), "Out", "Quad", 0.3, true)
intensity = 15
sound:Play()

-- Nền đổi trắng ↔ đen mỗi 0.2s trong 1 giây
for i = 1, 5 do
	bg.BackgroundColor3 = (i % 2 == 0) and Color3.new(0,0,0) or Color3.new(1,1,1)
	task.wait(0.2)
end

game.Players.LocalPlayer.Character.Humanoid.Health = 0
firesignal(game.ReplicatedStorage.RemotesFolder.DeathHint.OnClientEvent, {"Welcome back, what did you get now?","Hm.. I don't really know who you dead to","Maybe you can call him Darkness","Whenever he appears, you will hear the wind noises","Anyways you can do it! Bye!! :>"},"Blue")

game.ReplicatedStorage.GameStats["Player_".. game.Players.LocalPlayer.Name].Total.DeathCause.Value = "Darkness"

-- Stage 3: Fade out (mặt tan biến + nền mờ dần + rung giảm dần)
for i = 1, 20 do
	img.ImageTransparency = i / 20
	bg.BackgroundTransparency = i / 20
	intensity = math.max(0, 15 - i) -- giảm rung từ 15 về 0
	task.wait(0.05)
end

-- Kết thúc
shaking = false
gui:Destroy()
		end
	end
end)

--// Run entity
entity:Run()
