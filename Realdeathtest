local caption = game.Players.LocalPlayer.PlayerGui.MainUI.MainFrame.Caption
	caption.TextColor3 = Color3.fromRGB(255, 222, 189)

	require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game)
		.caption("Something is Coming i need to hide", true)

wait(3)

---====== Load spawner ======---

local spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/V2/Source.lua"))()

