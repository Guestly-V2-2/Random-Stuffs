function GitAud(soundgit,filename)
    SoundName=tostring(SoundName)
    local url=soundgit
    local FileName = filename
    writefile(FileName..".mp3", game:HttpGet(url))
    return (getcustomasset or getsynasset)(FileName..".mp3")
end

function CustomGitSound(soundlink, vol, filename)
    local sound = Instance.new("Sound")
    sound.SoundId = GitAud(soundlink, filename)
    sound.Parent = workspace
    sound.Name = "AmbientOfT2"
    sound.Volume = 1
   sound:Play()
end

CustomGitSound("https://github.com/Guestly-V2-2/Music-Sound/raw/refs/heads/main/YTSave.com_YouTube_CONCEPT-TOE1_Media_mXTYvRf3qew_002_720p.mp3?raw=true", 1, " TheRoastedOne")

local vm8 = true
wait(27)

task.spawn(function()
    while true do
        repeat
            wait(10000, 10000000)
        until not game.Players.LocalPlayer.Character:GetAttribute("Crounching") and vm8 == true
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Guestly-V2-2/Random-Stuffs/refs/heads/main/TBO%20JUMPSCARE"))()
    end
end)

wait(7)
game.Workspace.AmbientOfT2:Destroy()
